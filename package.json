{
  "name": "@ed.sahakian/theone",
  "version": "1.0.8",
  "description": "A simple JavaScript API wrapper for accesing the The One API https://the-one-api.dev/",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "scripts": {
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "dev": "microbundle --watch --tsconfig tsconfig.json --no-sourcemap",
    "publish": "npm publish --access=public"
  },
  "keywords": [],
  "author": "edster9",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "microbundle": "^0.15.1"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0"
  }
}
